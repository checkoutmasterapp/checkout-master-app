      .backend-admin-loader
        .backend-admin-loader-spinner
          .bar1
          .bar2
          .bar3
      header#header.header.fixed-top.d-flex.align-items-center

        div.d-flex.align-items-center.justify-content-between.nav-bar-resposive.header_inner
          a.logo.d-flex.align-items-center(
            href=(store_id?`/${store_id}/dashboard`:'/store-connect')
